<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference xml:lang="de-DE" id="r_lieferumfang">
	<title>Lieferumfang</title>
	<refbody>
		<section>
			<p>Die Grauwasseranlage <keyword keyref="prod-name"/> besteht aus folgenden Komponenten:</p>
		</section>
		<section otherprops="tank-gw">
			<title>Baugruppe <keyword keyref="group-num-gw"/></title>
			<ul>
				<li><keyword keyref="gw-tanks"/> x Behälter <keyword keyref="gw-tank-vol"/></li>
				<li><keyword keyref="gw-vents"/> x Belüfter</li>
				<li><keyword keyref="gw-pumps"/> x Förderpumpe</li>
			</ul>
		</section>
		<section otherprops="tank-gwf tank-f">
			<title>Baugruppe <keyword keyref="group-num-f"/></title>
			<ul>
				<li><keyword keyref="f-tanks"/> x Behälter <keyword keyref="f-tank-vol"/></li>
				<li><keyword keyref="f-membs"/> x Membraneinheit</li>
				<li><keyword keyref="f-vents"/> x Belüfter</li>
				<li><keyword keyref="f-perms"/> x Permeatpumpe</li>
			</ul>
		</section>
		<section>
			<title>Baugruppe <keyword keyref="group-num-sw"/></title>
			<ul>
				<li><keyword keyref="sw-tanks"/> x Behälter <keyword keyref="sw-tank-vol"/></li>
				<li>Trinkwassernachspeisung</li>
			</ul>
		</section>
		<section>
			<title>Baugruppe <keyword keyref="group-num-ctrl"/></title>
			<ul>
				<li>Steuerung</li>
			</ul>
		</section>
	</refbody>
</reference>
